        <fullName>clzStateIsSubmittedOrOpen</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
ISBLANK(Id) ,
NOT(ISPICKVAL(CLZ_State__c ,&quot;Submitted&quot;)),
NOT( ISPICKVAL(CLZ_State__c,&quot;Opened&quot;))
)</errorConditionFormula>
        <errorDisplayField>CLZ_State__c</errorDisplayField>
        <errorMessage>State can be &quot;Opened&quot; or &quot;Submitted&quot;</errorMessage>
    </validationRules>

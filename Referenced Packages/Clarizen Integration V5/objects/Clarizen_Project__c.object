    <validationRules>
        <fullName>PrecentIsLowerThen100</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
  (CLZ_PercentCompleted__c &gt; 1), 
  (CLZ_PercentCompleted__c &lt; 0)
)</errorConditionFormula>
        <errorDisplayField>CLZ_PercentCompleted__c</errorDisplayField>
        <errorMessage>% Complete should be between 0-100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StartIsBeforeEnd</fullName>
        <active>true</active>
        <errorConditionFormula>CLZ_StartDate__c &gt; CLZ_DueDate__c</errorConditionFormula>
        <errorDisplayField>CLZ_DueDate__c</errorDisplayField>
        <errorMessage>The project due date should not be before the start date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>StateIsOnlyDraftOrActive</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
  ISBLANK(Id) ,
  NOT(ISPICKVAL(CLZ_State__c,&quot;Active&quot;)), 
  NOT( ISPICKVAL(CLZ_State__c,&quot;Draft&quot;))
)</errorConditionFormula>
        <errorDisplayField>CLZ_State__c</errorDisplayField>
        <errorMessage>State can be Active or Draft</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>check_template_type</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
  NOT(ISBLANK( CLZ_Clarizen_Template__c )),
  CLZ_Clarizen_Template__r.TypeName__c &lt;&gt; &quot;Project&quot;
)</errorConditionFormula>
        <errorDisplayField>CLZ_Clarizen_Template__c</errorDisplayField>
        <errorMessage>Template TypeName should be &quot;Project&quot;</errorMessage>
    </validationRules>

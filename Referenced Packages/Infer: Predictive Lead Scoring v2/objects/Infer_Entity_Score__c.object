    <fields>
        <fullName>Band__c</fullName>
        <deprecated>false</deprecated>
        <description>The Infer Score, put into bands with range of 10 each.</description>
        <externalId>false</externalId>
        <formula>IF(ISBLANK(Score__c), NULL,
IF(Score__c &lt; 10, &quot;0 - 10&quot;,
IF(Score__c &gt;= 10 &amp;&amp; Score__c &lt; 20, &quot;10 - 20&quot;,
IF(Score__c &gt;= 20 &amp;&amp; Score__c &lt; 30, &quot;20 - 30&quot;,
IF(Score__c &gt;= 30 &amp;&amp; Score__c &lt; 40, &quot;30 - 40&quot;,
IF(Score__c &gt;= 40 &amp;&amp; Score__c &lt; 50, &quot;40 - 50&quot;,
IF(Score__c &gt;= 50 &amp;&amp; Score__c &lt; 60, &quot;50 - 60&quot;,
IF(Score__c &gt;= 60 &amp;&amp; Score__c &lt; 70, &quot;60 - 70&quot;,
IF(Score__c &gt;= 70 &amp;&amp; Score__c &lt; 80, &quot;70 - 80&quot;,
IF(Score__c &gt;= 80 &amp;&amp; Score__c &lt; 90, &quot;80 - 90&quot;,
IF(Score__c &gt;= 90, &quot;90 - 100&quot;, NULL)))))))))))</formula>
        <inlineHelpText>The Infer Score, put into bands with range of 10 each.</inlineHelpText>
        <label>Band</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

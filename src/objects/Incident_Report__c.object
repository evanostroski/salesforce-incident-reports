    <validationRules>
        <fullName>Affected_Data_Centers_Required</fullName>
        <active>true</active>
        <errorConditionFormula>ISNULL( Affected_Data_Centers__c )</errorConditionFormula>
        <errorDisplayField>Affected_Data_Centers__c</errorDisplayField>
        <errorMessage>Must Select Affected Data Centers</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Time_Required_on_Resolved</fullName>
        <active>true</active>
        <errorConditionFormula>OR( 
AND(ISPICKVAL( Status__c , &apos;Open&apos;), NOT(ISNULL( End_Time__c ))) ,
AND(ISPICKVAL( Status__c , &apos;Resolved&apos;), ISNULL( End_Time__c )), AND(ISPICKVAL( Status__c , &apos;Closed&apos;) , ISNULL( End_Time__c )))</errorConditionFormula>
        <errorDisplayField>Affected_Data_Centers__c</errorDisplayField>
        <errorMessage>Check that Incident&apos;s Status and End Time match process</errorMessage>
    </validationRules>
    <webLinks>
        <fullName>Send_Post_Mortem</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Send Post Mortem</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/20.0/apex.js&quot;)}

var t = {!ISPICKVAL(Incident_Report__c.Status__c, &apos;Closed&apos;)};

if( t ) {
window.open(&apos;/apex/IR_Send_PostMortem?id={!Incident_Report__c.Id}&apos;);
} else {
alert (&quot;Status must be Closed in order to send Post Mortem&quot;);
}</url>
    </webLinks>
